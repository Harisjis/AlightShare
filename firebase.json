{
  "hosting": {
  "target": "alightshare",
    "public": "public",
    "ignore": [
      "firebase.json",
      ".firebaserc",
      "README.md",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
